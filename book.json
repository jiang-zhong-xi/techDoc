{
	"title": "个人技术文档",
	"description": "This is such a great book!",
	"plugins": [
         "expandable-chapters", "page-treeview", "favicon", "hide-element", "-sharing", "-lunr", "-search", "search-pro"
    ],
	"pluginsConfig": {
        "page-treeview": {
            "copyright": "Copyright &#169; aleen42",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
		"favicon": {
            "shortcut": "asset/img/favicon.ico",
            "bookmark": "asset/img/favicon.ico",
            "appleTouch": "asset/img/favicon.ico",
            "appleTouchMore": {
                "120x120": "asset/img/favicon.ico",
                "180x180": "asset/img/favicon.ico"
            }
        },
		"hide-element": {
            "elements": [".gitbook-link"]
        }
    },
	"styles": {
		"website": "asset/css/website.css"
	}
}